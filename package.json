{
  "name": "bfs-calendar-mocks",
  "version": "1.0.0",
  "description": "Mocks for BFS Calendar",
  "main": "src/index.js",
  "scripts": {
    "rm_install": "rm -rf ./node_modules && npm install",
    "build": "npm run tsc",
    "start": "npm run build && ./node_modules/babel-cli/bin/babel-node.js --presets node6 ./src/index.js",
    "tsc": "tsc --pretty",
    "mocks": "cd scripts && chmod +x restart-bfs-calendar-mocks-remotely.sh && ./restart-bfs-calendar-mocks-remotely.sh"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.0.39",
    "body-parser": "~1.0.1",
    "express": "~4.0.0",
    "moment": "^2.21.0",
    "typescript": "2.3.3",
    "typings": "^2.1.1"
  },
  "author": "IScream",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-node6": "^11.0.0"
  }
}
